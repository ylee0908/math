https://leetcode.com/problems/power-of-three/


class Solution:
    def isPowerOfThree(self, n: int) -> bool:
        if n <= 0: 
            return False
        while n % 3 == 0:
            n = n/3
        return n== 1
    

class Solution(object):
    def isPowerOfThree(self, n):
        return n > 0 and 1162261467 % n == 0
        #1162261467 = floor( LARGEST_INT_SUPPORTED / 3 ) * 3
